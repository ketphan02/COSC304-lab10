<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />

    <style>
      table, th, td { border: 1px solid; }
    </style>
  </head>

  <body>
    <title>Kiet Phan Grocery</title>
    <h1>{{title}}</h1>
    <table>
      <tr>
        <th>Username</th>
        <td>{{username}}</td>
      </tr>
      <tr>
        <th>First name</th>
        <td>{{firstName}}</td>
      </tr>
      <tr>
        <th>Last name</th>
        <td>{{lastName}}</td>
      </tr>
      <tr>
        <th>Email</th>
        <td>{{email}}</td>
      </tr>
      <tr>
        <th>Phone</th>
        <td>{{phone}}</td>
      </tr>
      <tr>
        <th>Address</th>
        <td>{{address}}</td>
      </tr>
      <tr>
        <th>City</th>
        <td>{{city}}</td>
      </tr>
      <tr>
        <th>State</th>
        <td>{{state}}</td>
      </tr>
      <tr>
        <th>Zip</th>
        <td>{{zip}}</td>
      </tr>
      <tr>
        <th>Country</th>
        <td>{{country}}</td>
      </tr>
      <tr>
        <th>User ID</th>
        <td>{{userid}}</td>
      </tr>
  </body>

</html>